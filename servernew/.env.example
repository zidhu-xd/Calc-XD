# Environment variables for Calc-XD Server
# Copy this file to .env and fill in the values

# Database connection URL (Required)
# For Render PostgreSQL, this will be provided in the Render dashboard
# Format: postgres://user:password@host:port/database
DATABASE_URL=

# Server port (Optional, defaults to 5000)
PORT=5000

# Node environment (Optional, defaults to development)
# Set to 'production' for production deployments
NODE_ENV=development

# Allowed origins for CORS (Optional)
# Comma-separated list of allowed origins
# Example: https://myapp.com,https://www.myapp.com
ALLOWED_ORIGINS=
